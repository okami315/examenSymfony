{% extends 'base.html.twig' %}

{% block title %}Salidas{% endblock %}

{% block body %}
    <h1>Salidas</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ salida.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ salida.quantity }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ salida.date ? salida.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_salidas_index') }}">back to list</a>

    <a href="{{ path('app_salidas_edit', {'id': salida.id}) }}">edit</a>

    {{ include('salidas/_delete_form.html.twig') }}
{% endblock %}
